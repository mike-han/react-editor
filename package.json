{
  "name": "react-editor",
  "version": "1.0.0",
  "description": "A react component reply on quill.js",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "rimraf umd && npm run build-umd && npm run build-min",
    "build-umd": "cross-env NODE_ENV=development webpack src/index.tsx -o umd/react-editor.js",
    "build-min": "cross-env NODE_ENV=production webpack src/index.tsx -o umd/react-editor.min.js",
    "demo": "webpack-dev-server --progress --host 127.0.0.1 --inline --config demo/webpack.config.js",
    "test": "cross-env NODE_ENV=test karma start test/karma.conf.js",
    "lint": "eslint './src/**/*.?(ts|tsx)'",
    "autofix": "eslint './src/**/*.?(ts|tsx)' --fix",
    "analyse": "cross-env NODE_ENV=analyse webpack src/index.ts -o umd/Recharts.js",
    "tsc": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mike-han/react-editor.git"
  },
  "author": "mike-han",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mike-han/react-editor/issues"
  },
  "homepage": "https://github.com/mike-han/react-editor#readme",
  "files": [
    "umd/",
    "lib/",
    "types.d.ts",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@types/lodash-es": "^4.17.3",
    "@types/react": "^16.9.56",
    "copy-webpack-plugin": "^6.3.1",
    "cross-env": "^7.0.2",
    "html-webpack-plugin": "^4.5.0",
    "react-dom": "^17.0.1",
    "rimraf": "^3.0.2",
    "ts-loader": "^8.0.10",
    "typescript": "^4.0.5",
    "webpack": "^5.4.0",
    "webpack-bundle-analyzer": "^4.1.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "lodash-es": "^4.17.15",
    "quill": "^1.3.7",
    "react": "^17.0.1"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  }
}
